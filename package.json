{
  "name": "dawn-naglich-wellness",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "pnpm --filter functions build && pnpm build:web --clear",
    "build:dev": "pnpm build --dev",
    "build:web": "expo export --platform web --output-dir dist && node webpack.config.js dist",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "deploy": "pnpm deploy:all",
    "deploy:all": "pnpm exec firebase deploy --only functions,hosting",
    "deploy:web": "pnpm build:web && pnpm exec firebase deploy --only hosting",
    "deploy:functions": "pnpm --filter functions deploy",
    "screenshot:login": "node scripts/screenshot-login.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@google/genai": "^1.34.0",
    "expo": "~54.0.30",
    "expo-asset": "^12.0.12",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-constants": "^18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "^19.0.21",
    "expo-gl": "^16.0.9",
    "expo-linking": "~8.0.11",
    "expo-router": "^6.0.21",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "^15.0.10",
    "firebase": "^10.14.1",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.6",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "^0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "3.3.2",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.28.4",
    "@types/react": "~19.1.17",
    "puppeteer": "^23.11.1",
    "typescript": "~5.9.3"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "protobufjs"
    ],
    "onlyBuiltDependencies": [
      "puppeteer"
    ]
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}

{
  "name": "dawn-naglich-wellness",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "all || pnpm build:firebase && echo 'Build Complete'",
    "build:firebase": "pnpm --filter functions build && pnpm build:web --clear",
    "build:dev": "pnpm build --dev",
    "build:eas": "eas build --profile production",
    "build:web": "expo export --platform web --output-dir dist && node webpack.config.js dist",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "deploy": "pnpm deploy:firebase",
    "deploy:all": "pnpm deploy:firebase && pnpm deploy:eas",
    "deploy:firebase": "pnpm exec firebase deploy --only functions,hosting",
    "deploy:eas": "eas deploy --profile production --wait",
    "deploy:web": "pnpm build:web && pnpm exec firebase deploy --only hosting",
    "deploy:functions": "pnpm --filter functions deploy",
    "screenshot:login": "node scripts/screenshot-login.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@google/genai": "^1.34.0",
    "expo": "~54.0.30",
    "expo-asset": "^12.0.12",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-constants": "^18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.10",
    "expo-gl": "^16.0.9",
    "expo-linking": "~8.0.11",
    "expo-router": "^6.0.21",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "^15.0.10",
    "firebase": "^10.14.1",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.6",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "^0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "3.3.2",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/runtime": "^7.28.4",
    "@eslint/css": "^0.14.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@types/react": "~19.1.17",
    "babel-preset-expo": "^54.0.9",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "puppeteer": "^24.34.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "protobufjs"
    ],
    "onlyBuiltDependencies": [
      "puppeteer"
    ]
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}

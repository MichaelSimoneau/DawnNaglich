{
  "name": "dawn-naglich-wellness",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "pnpm --filter functions build && pnpm build:web --clear",
    "build:dev": "pnpm build --dev",
    "build:web": "expo export --platform web --output-dir dist",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "deploy": "pnpm deploy:all",
    "deploy:all": "pnpm exec firebase deploy --only functions,hosting",
    "deploy:web": "pnpm build:web && pnpm exec firebase deploy --only hosting",
    "deploy:functions": "pnpm --filter functions deploy"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@google/genai": "^1.34.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "expo": "~51.0.0",
    "expo-asset": "^10.0.10",
    "expo-auth-session": "~5.5.2",
    "expo-av": "~14.0.5",
    "expo-constants": "^16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-file-system": "^17.0.1",
    "expo-gl": "^14.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "^3.5.24",
    "expo-status-bar": "~1.12.1",
    "firebase": "^10.12.0",
    "nativewind": "^4.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-reanimated": "^3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.13",
    "react-native-worklets": "^0.7.1",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "3.3.2",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.28.4",
    "@types/react": "~18.2.79",
    "typescript": "~5.3.3"
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0"
    },
    "ignoredBuiltDependencies": [
      "protobufjs"
    ]
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}

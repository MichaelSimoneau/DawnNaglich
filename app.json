{
  "expo": {
    "name": "Dawn Naglich",
    "slug": "dawn-naglich",
    "scheme": "dawn-naglich",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#173129"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dawnnaglich.wellness",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleDisplayName": "Dawn Naglich",
        "NSMicrophoneUsageDescription": "Dawn needs microphone access for the Becky AI scheduling assistant.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#173129"
      },
      "package": "com.dawnnaglich.wellness",
      "permissions": [
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "baseUrl": "/"
    },
    "extra": {
      "eas": {
        "projectId": "16065a72-fc14-4c7c-8feb-2bb5a44b2616"
      },
      "router": {}
    },
    "plugins": [
      "expo-asset",
      "expo-router",
      "expo-font",
      [
        "@react-native-firebase/app",
        {
          "iosGoogleServicesFile": "./ios/DawnNaglichWellness/GoogleServices-Info.plist",
          "androidGoogleServicesFile": "./android/app/google-services.json"
        }
      ],
      "./expo-audio-streaming/app.plugin.js"
    ],
    "owner": "michael-simoneau"
  }
}
